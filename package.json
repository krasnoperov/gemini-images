{
  "name": "@krasnoperov/gemini-images",
  "version": "1.1.0",
  "description": "Consistent image generation with Gemini - Create 2D game sprites, characters, and environments with visual consistency using reference sheets methodology",
  "type": "module",
  "bin": {
    "gemini-images": "./dist/cli.js"
  },
  "scripts": {
    "dev": "node src/cli.ts",
    "build": "rollup -c",
    "typecheck": "tsc --noEmit",
    "test": "node --test 'test/**/*.test.ts'",
    "lint": "eslint . --fix"
  },
  "keywords": [
    "gemini",
    "claude-skills",
    "game-development",
    "image-generation",
    "ai",
    "cli"
  ],
  "license": "MIT",
  "author": "Aleksei Krasnoperov",
  "repository": {
    "type": "git",
    "url": "https://github.com/krasnoperov/gemini-images.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "skills/",
    ".claude-plugin/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@google/generative-ai": "0.24.1",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-typescript": "12.3.0",
    "@stylistic/eslint-plugin": "5.6.1",
    "@types/node": "24.10.1",
    "eslint": "9.39.1",
    "rollup": "4.53.3",
    "semantic-release": "24.2.6",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0"
  }
}
